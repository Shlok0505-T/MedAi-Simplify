{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "./",
    "MEDICAL_API_URL": "",
    "API_KEY": ""
  },
  "functions": {
    "main.py": {
      "maxDuration": 30
    }
  }
}